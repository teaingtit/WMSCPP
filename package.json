{
  "name": "my-wms-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-http-header-size=65536 next dev -p 3006",
    "dev:turbo": "cross-env NODE_OPTIONS=--max-http-header-size=65536 next dev -p 3006 --turbopack",
    "build": "next build",
    "start": "cross-env NODE_OPTIONS=--max-http-header-size=65536 node .next/standalone/server.js",
    "start:dev": "cross-env NODE_OPTIONS=--max-http-header-size=65536 next start",
    "lint": "eslint \"src/app/**/*.{js,jsx,ts,tsx}\" \"src/components/**/*.{js,jsx,ts,tsx}\" \"src/lib/**/*.{js,jsx,ts,tsx}\" \"src/actions/**/*.{js,jsx,ts,tsx}\" \"src/types/**/*.{js,jsx,ts,tsx}\" --cache",
    "lint:clean": "eslint \"src/app/**/*.{js,jsx,ts,tsx}\" \"src/components/**/*.{js,jsx,ts,tsx}\" \"src/lib/**/*.{js,jsx,ts,tsx}\" \"src/actions/**/*.{js,jsx,ts,tsx}\" \"src/types/**/*.{js,jsx,ts,tsx}\" --no-cache",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "check": "npx tsc --noEmit && npm run lint",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "predeploy": "npm run build",
    "test:integration": "playwright test -c playwright.integration.config.ts",
    "tsc:strict": "tsc -p tsconfig.strict.json --noEmit",
    "clean": "node -e \"const fs=require('fs');['.next','coverage','node_modules/.cache','node_modules/.vite','.vitest','.eslintcache','tsconfig.tsbuildinfo','tsconfig.test.tsbuildinfo','tsconfig.strict.tsbuildinfo','test-results','playwright-report','e2e-results'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true});console.log('Removed:',d)}catch(e){}})\"",
    "clean:reinstall": "npm run clean && npm ci"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@google/generative-ai": "^0.11.5",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.43.4",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/lodash-es": "^4.17.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parser": "^3.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.22",
    "lucide-react": "^0.378.0",
    "next": "^16.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.6.0",
    "use-debounce": "^10.0.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.19",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "husky": "^8.0.3",
    "jsdom": "^27.4.0",
    "lint-staged": "^13.2.3",
    "playwright": "^1.57.0",
    "postcss": "^8.4.38",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2",
    "typescript": "5.3.3",
    "vitest": "^4.0.17"
  },
  "overrides": {
    "micromatch": "4.0.8"
  }
}
